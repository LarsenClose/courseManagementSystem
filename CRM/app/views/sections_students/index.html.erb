<p id="notice"><%= notice %></p>

<h1>Sections Students</h1>

<table>
  <thead>
    <tr>
      <th>Section</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sections_students.each do |sections_student| %>
      <tr>
        <td><%= sections_student.section_id %></td>
        <td><%= sections_student.student_id %></td>
        <td><%= link_to 'Show', sections_student %></td>
        <td><%= link_to 'Edit', edit_sections_student_path(sections_student) %></td>
        <td><%= link_to 'Destroy', sections_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sections Student', new_sections_student_path %>
